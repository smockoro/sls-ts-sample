{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-duplicate-imports": {
            "severity": "off"
        },
        "@typescript-eslint/no-duplicate-imports": ["error"],
        "prettier": [
            true, {
                "printWidth": 80,
                "tabWidth": 2,
                "useTabs": false,
                "semi": true,
                "singleQuote": true,
                "bracketSpacing": true,
                "arrowParens": "always",
                "endOfLine": "auto"
            }
        ]
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}