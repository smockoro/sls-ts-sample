{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": [
            true, {
                "printWidth": 80,
                "tabWidth": 2,
                "useTabs": false,
                "semi": true,
                "singleQuote": true,
                "bracketSpacing": true,
                "arrowParens": "always"
            }
        ]
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}